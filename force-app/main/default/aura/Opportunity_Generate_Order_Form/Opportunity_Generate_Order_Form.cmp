<aura:component implements="force:lightningQuickActionWithoutHeader,force:hasRecordId"><aura:attribute name="isGenerating" type="Boolean" default="{!false}"/><aura:attribute name="finishedGenerating" type="Boolean" default="{!false}"/><aura:attribute name="disableGenerateButton" type="Boolean" default="{!false}"/><aura:attribute name="errMsg" type="Boolean"/><div aura:id="scope" class="ds-scope"><div class="slds-grid slds-grid_vertical"><div class="slds-modal__header"><h2 class="slds-text-heading_medium slds-hyphenate">Generate Documents</h2></div><div class="slds-modal__content slds-grow slds-is-relative slds-p-around_medium"><dsgen:generator aura:id="generator" templateId="a092M00001IfNYtQAN" recordId="{!v.recordId}" finishedGenerating="{!v.finishedGenerating}" isGenerating="{!v.isGenerating}" disableGenerateButton="{!v.disableGenerateButton}" errMsg="{!v.errMsg}"/></div><aura:if isTrue="{!empty(v.errMsg)}"><div class="slds-modal__footer"><aura:if isTrue="{!not(v.finishedGenerating)}"><lightning:button label="Cancel" disabled="{!v.isGenerating}" onclick="{!c.close}" /><lightning:button variant="brand" label="Generate" disabled="{!or(v.isGenerating, v.disableGenerateButton)}" onclick="{!c.generateDocs}"/></aura:if><aura:if isTrue="{!v.finishedGenerating}"><lightning:button variant="brand" label="Done" onclick="{!c.close}"/> </aura:if></div></aura:if></div></div></aura:component>